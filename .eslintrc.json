{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2024,
    "sourceType": "module",
    "project": "tsconfig.json"
  },
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
  "env": {
    "node": true,
    "jest/globals": true
  },
  "rules": {
    //'no-console': 'off',
    //'import/prefer-default-export': 'off',
    "@typescript-eslint/no-unused-vars": "warn"
  },
  "ignorePatterns": ["dist"]
}
